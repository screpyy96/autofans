VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Optional: if you prefer Remix-style public vars
# (the code supports both VITE_* and REMIX_PUBLIC_*). For Vite
# to expose to the browser, VITE_* is required, so best to keep both.
REMIX_PUBLIC_SUPABASE_URL=
REMIX_PUBLIC_SUPABASE_ANON_KEY=

# Set this to your app origin for local dev if you use any absolute URLs
# For local dev with react-router dev (Vite), it's usually http://localhost:5173
# VITE_SITE_URL=http://localhost:5173
